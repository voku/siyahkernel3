188a189,207
> uint8 * BCMFASTPATH
> pktoffset(osl_t *osh, void *p,  uint offset)
> {
> 	uint total = pkttotlen(osh, p);
> 	uint pkt_off = 0, len = 0;
> 	uint8 *pdata = (uint8 *) PKTDATA(osh, p);
> 
> 	if (offset > total)
> 		return NULL;
> 
> 	for (; p; p = PKTNEXT(osh, p)) {
> 		pdata = (uint8 *) PKTDATA(osh, p);
> 		pkt_off = offset - len;
> 		len += PKTLEN(osh, p);
> 		if (len > offset)
> 			break;
> 	}
> 	return (uint8*) (pdata+pkt_off);
> }
