> int wl_cfg80211_do_driver_init(struct net_device *net)
> {
> 	struct wl_priv *wl = *(struct wl_priv **)netdev_priv(net);
> 
> 	if (!wl || !wl->wdev)
> 		return -EINVAL;
> 
> 	if (dhd_do_driver_init(wl->wdev->netdev) < 0)
> 		return -1;
> 
> 	return 0;
> }
